{
    "name": "phalconvee/joker",
    "description": "My chuck norris joker jokes",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Henry Ugochukwu",
            "email": "phalconvee@gmail.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "PhalconVee\\Joker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhalconVee\\Joker\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "orchestra/testbench": "^6.4"
    },
    "extra": {
        "laravel": {
            "providers": [
                "PhalconVee\\Joker\\ChuckNorrisJokesServiceProvider"
            ],
            "aliases": {
                "ChuckNorris": "PhalconVee\\Joker\\Facades\\ChuckNorris"
            }
        }
    }
}
